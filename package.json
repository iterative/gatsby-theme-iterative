{
  "name": "gatsby-theme-iterative-workspace",
  "private": true,
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "check-format": "prettier -c \"**/*.{js,json,ts,tsx,css,md,mdx}\"",
    "check-lint": "eslint \"**/*.{js,jsx,ts,tsx,json}\"",
    "format-staged": "pretty-quick --staged --no-restage --bail --pattern '**/*.{js,jsx,md,tsx,ts,json}'",
    "format-all": "prettier --write '**/*.{js,jsx,md,tsx,ts,json,css}'",
    "format": "prettier --write",
    "lint-ts": "tsc --noEmit --skipLibCheck && eslint --ext .json,.js,.ts,.tsx .",
    "lint-css": "stylelint \"**/src/**/*.css\"",
    "clean": "yarn workspace example clean",
    "build": "yarn workspace example build",
    "serve": "yarn workspace example serve",
    "develop": "yarn workspace example develop",
    "get-commands": "yarn workspace @dvcorg/gatsby-theme-iterative get-commands",
    "prepare": "husky install"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "gatsby-plugin-postcss": "^6.4.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.1",
    "prettier": "2.8.4",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "29.0.0"
  }
}
